<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pengembalian">

    <TableLayout
        android:id="@+id/tableLayout4"
        android:layout_width="294dp"
        android:layout_height="246dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.148">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblLocker"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Locker"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/textLocker"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="TextView"
                android:textColor="#333" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblPutTime"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Put Time"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/textPutTime"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="TextView"
                android:textColor="#333" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblTakeTime"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Take Time"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/textTakeTime"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="TextView"
                android:textColor="#333" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblDuration"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Duration"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/textDuration"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="TextView"
                android:textColor="#333" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblPrice"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Price"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/textPrice"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="TextView"
                android:textColor="#333" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/lblPayment"
                android:layout_width="wrap_content"
                android:layout_height="15pt"
                android:text="Payment"
                android:textColor="#333"
                android:textSize="8pt" />

            <TextView
                android:id="@+id/lblFlazz"
                android:layout_width="match_parent"
                android:layout_height="15pt"
                android:text="FlazzBCA"
                android:textColor="#333" />

        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Restore Page"
        android:textColor="#333"
        android:textSize="12pt"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="140dp"
        android:text="Pay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout4" />
</androidx.constraintlayout.widget.ConstraintLayout>